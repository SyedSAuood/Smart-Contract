pragma solidity ^0.8.10;

contract calculater{
    uint8 num1;
    uint8 num2;
   
    function setnum(uint8 _num1 , uint8 _num2) public {
        num1 = _num1;
        num2 = _num2;
    }

    function Add() view public returns(uint8 res){
        res = num1 + num2;
        return(res);
    }

     function Sub() view public returns(uint8 res){
        res = num1 - num2;
        return(res);
    }

    function Mul() view public returns(uint8 res){
        res = num1 * num2;
        return(res);
    }

    function Div() view public returns(uint8 res){
        res = num1 / num2;
        return(res);
    }

}
